{
    "name": "antwebes/chat-boilerplate",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-0": {
            "": "src/"
        }
    },
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "2.3.*",
        "doctrine/orm": "~2.2,>=2.2.3,<2.5",
        "doctrine/dbal": "<2.5",
        "doctrine/doctrine-bundle": "~1.2",
        "twig/extensions": "1.0.*",
        "symfony/assetic-bundle": "~2.3",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~2.3",
        "sensio/framework-extra-bundle": "~2.3",
        "sensio/generator-bundle": "~2.3",
        "incenteev/composer-parameter-handler": "~2.0",
        "antwebes/api-social-bundle": "dev-mans_networking",
        "raulfraile/ladybug-bundle": "~1.0",
        "predis/predis": "^1.0",
        "snc/redis-bundle": "1.1.x-dev",
        "antwebes/help-bundle": "dev-mans_boilerplate",
        "antwebes/pretty-bundle": "dev-mans_boilerplate",
        "jms/translation-bundle": "1.1.0",
        "stof/doctrine-extensions-bundle": "^1.2",
        "javiereguiluz/easyadmin-bundle": "^1.15",
        "mremi/contact-bundle": "v1.3.0",
        "vich/uploader-bundle": "v0.14.0",
        "dizda/cloud-backup-bundle": "^3.4",
        "knplabs/knp-gaufrette-bundle": "~0.3",
        "aws/aws-sdk-php" : "~2.8"
    },
    "require-dev" : {
        "behat/behat" : "2.4.*@stable",
        "behat/mink" : "1.4.*@stable",
        "behat/mink-extension" : "*",
        "behat/mink-goutte-driver" : "*",
        "behat/mink-selenium2-driver" : "*",
        "behat/symfony2-extension": "*",
        "antwebes/guzzle-fake-server" : "dev-master"
    },
    "repositories" : [{
        "type" : "git",
        "url" : "git@bitbucket.org:antweb/prettybundle.git"
    }],
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "AppBundle\\Provider\\Composer\\ParameterGenerator::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
